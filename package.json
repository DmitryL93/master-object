{
  "name": "master-object",
  "version": "1.0.0",
  "description": "Protects your code from typeErrors when nesting in deep objects and arrays",
  "scripts": {
    "build": "tsup ./src",
    "format": "prettier --write src tests",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DmitryL93/master-object.git"
  },
  "bugs": {
    "url": "https://github.com/DmitryL93/master-object/issues"
  },
  "keywords": [
    "object",
    "array",
    "deep",
    "path",
    "key",
    "keys",
    "keypath",
    "typeError",
    "inspect"
  ],
  "author": "Dmitry Loboda <dmitry.loboda.93@gmail.com>",
  "license": "MIT",
  "files": [
    "dist",
    "package.json"
  ],
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    }
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "devDependencies": {
    "@types/jest": "^29.5.10",
    "@types/lodash.clonedeep": "^4.5.9",
    "jest": "^29.7.0",
    "prettier": "^3.1.0",
    "ts-jest": "^29.1.1",
    "tsup": "^8.0.1",
    "typescript": "^5.3.2"
  },
  "dependencies": {
    "lodash.clonedeep": "^4.5.0"
  }
}
